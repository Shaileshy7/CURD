<h1>All Users</h1>
<a href="/create">Create New</a>
<ul>
    <% users.forEach(user => { %>
        <li>
            <%= user.name %> - <%= user.email %> - <%= user.age %>
         
            <a href="/edit/<%= user._id %>">Edit</a>
            <a href="/delete/<%= user._id %>">Delete</a>
        
        </li>
    <% }) %>
</ul>